package fibonacci;
import java.util.*;
public class Fibonacci {
    public static void main(String[] args) {
        int a = 0, b = 1, c;
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number :: ");
        int num = sc.nextInt();
        System.out.print(a+" "+b);
        for(int i = 0; i <= num; i++)
        {
            c = a + b;
            System.out.print(" "+c);
            a = b;
            b = c;
        }
    } 
}
